{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "homepage": "/",
    "dependencies": {
        "@draft-js-plugins/alignment": "^5.0.3",
        "@draft-js-plugins/drag-n-drop": "^4.2.2",
        "@draft-js-plugins/drag-n-drop-upload": "^4.2.3",
        "@draft-js-plugins/editor": "^4.1.3",
        "@draft-js-plugins/focus": "^4.1.3",
        "@draft-js-plugins/image": "^4.1.3",
        "@draft-js-plugins/resizeable": "^5.0.3",
        "@draft-js-plugins/static-toolbar": "^4.1.3",
        "@emotion/react": "^11.9.0",
        "@emotion/styled": "^11.8.1",
        "@material-ui/core": "^4.12.4",
        "@material-ui/icons": "^4.11.3",
        "@mui/icons-material": "^5.8.2",
        "@mui/lab": "latest",
        "@mui/material": "^5.8.1",
        "@mui/styles": "^5.8.0",
        "axios": "^0.23.0",
        "bootstrap": "^5.0.2",
        "dompurify": "^2.3.8",
        "draft-convert": "^2.1.12",
        "draft-js": "^0.11.7",
        "draftjs-to-html": "^0.9.1",
        "env-cmd": "^10.1.0",
        "eventing-bus": "^2.0.1",
        "lodash": "^4.17.21",
        "moment": "^2.29.3",
        "mui-datatables": "^4.2.2",
        "node-sass": "^6.0.1",
        "qs": "^6.11.0",
        "react": "^17.0.2",
        "react-bootstrap": "^2.0.0-beta.4",
        "react-dom": "^17.0.2",
        "react-error-overlay": "^6.0.9",
        "react-icons": "^4.2.0",
        "react-infinite-scroll-component": "^6.1.0",
        "react-moment": "^1.1.2",
        "react-redux": "^8.0.2",
        "react-router-config": "^5.1.1",
        "react-router-dom": "^6.3.0",
        "react-scripts": "^5.0.0",
        "react-tooltip": "^4.2.8",
        "redux": "^4.2.0",
        "redux-devtools-extension": "^2.13.9",
        "redux-thunk": "^2.4.1",
        "typescript": "^4.3.5"
    },
    "scripts": {
        "start": "env-cmd -f ./.env/.env.dev react-scripts start",
        "build": "env-cmd -f ./.env/.env.stag react-scripts build",
        "build.prod": "env-cmd -f ./.env/.env.prod react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "predeploy": "npm run build",
        "lint": "eslint ./ --ignore-path .gitignore",
        "lint:fix": "npm run lint -- --fix",
        "format": "prettier --write \"{,!(node_modules)/**/}*.tsx\""
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "rules": {
            "no-unused-vars": 0,
            "@typescript-eslint/no-unused-vars": 0,
            "react-hooks/exhaustive-deps": 0
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^11.2.7",
        "@testing-library/user-event": "^12.8.3",
        "@types/jest": "^26.0.24",
        "@types/node": "^12.20.19",
        "@types/react": "^17.0.18",
        "@types/react-dom": "^17.0.9",
        "@types/react-lottie": "^1.2.6",
        "@types/react-redux": "^7.1.24",
        "@types/react-router-dom": "^5.1.8",
        "@types/redux": "^3.6.0",
        "@types/redux-thunk": "^2.1.0",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "@typescript-eslint/parser": "^5.27.0",
        "eslint": "^8.9.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.26.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "generate-react-cli": "^7.0.4",
        "gh-pages": "^3.2.3",
        "prettier": "^2.4.1",
        "utility-types": "^3.10.0",
        "web-vitals": "^1.1.2"
    },
    "peerDependencies": {
        "@material-ui/core": "^4.12.4"
    }
}
